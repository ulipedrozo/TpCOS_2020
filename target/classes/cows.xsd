<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.isistan.edu.ar/springsoap/gen"
           targetNamespace="http://www.isistan.edu.ar/springsoap/gen" elementFormDefault="qualified">
 
    

    <xs:complexType name="Cow">
    	<xs:sequence>
    		<xs:element name="id" type="xs:int"></xs:element>
    		<xs:element name="weight" type="xs:decimal"></xs:element>
    		<xs:element name="electronic_id" type="xs:int"></xs:element>
    		<xs:element name="birthday" type="xs:date"></xs:element>
    		<xs:element name="last_delivery" type="xs:date"></xs:element>
    		<xs:element name="delivery_quantity" type="xs:int"></xs:element>
    	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="Herd">
    	<xs:sequence>
    		<xs:element name="id" type="xs:int"></xs:element>
    		<xs:element name="cows" type="tns:Cow" maxOccurs="unbounded"
    			minOccurs="1">
    		</xs:element>
    		<xs:element name="location" type="xs:string"></xs:element>
    	</xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CowBcs">
    	<xs:sequence>
    		<xs:element name="id" type="xs:int"></xs:element>
    		<xs:element name="cow_id" type="tns:Cow" maxOccurs="1"
    			minOccurs="1">
    		</xs:element>
    		<xs:element name="date" type="xs:date"></xs:element>
    		<xs:element name="cc" type="xs:float"></xs:element>
    	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="CowAlert">
    	<xs:sequence>
    		<xs:element name="id" type="xs:int"></xs:element>
    		<xs:element name="cow_id" type="tns:Cow"></xs:element>
    		<xs:element name="bcs_threshold" type="xs:float"></xs:element>
    	</xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="HerdAlert">
    	<xs:sequence>
    		<xs:element name="id" type="xs:int"></xs:element>
    		<xs:element name="herd_id" type="tns:Herd"></xs:element>
    		<xs:element name="bcs_threshold" type="xs:float"></xs:element>
    	</xs:sequence>
    </xs:complexType>

    <xs:element name="getHerdResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="herd" type="tns:Herd"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getHerdRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="saveCowRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="electronic-Id" type="xs:integer"/>
                <xs:element name="fecha_nacimiento" type="xs:date"/>
                <xs:element name="ultima_fecha_parto" type="xs:date"/>
                <xs:element name="cantidad_partos" type="xs:integer"/>
                <xs:element name="peso" type="xs:float"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="saveCowResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="cow" type="tns:Cow"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="getCowByEIDResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="cow" type="tns:Cow"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getCowByEIDRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="eid" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="addCowToHerdRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="cowId" type="xs:integer"/>
                <xs:element name="herdId" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="addCowToHerdResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="herd" type="tns:Herd"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="addCowBCSRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:integer"/>
                <xs:element name="cow_id" type="xs:integer"/>
                <xs:element name="date" type="xs:date"/>
                <xs:element name="cc" type="xs:float"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="addCowBCSResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="cowBcs" type="tns:CowBcs"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="getCowInfoRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="getCowInfoResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="herd" type="tns:Herd"/>
                <xs:element name="lastBCS" type="tns:CowBcs"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="getHerdInfoRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="getHerdInfoResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="herd" type="tns:Herd"/>
                <xs:element name="averageBCS" type="tns:CowBcs"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="setCowAlertRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="setCowAlertResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="cowAlert" type="tns:CowAlert"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="setHerdAlertRequest">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="setHerdAlertResponse">
    	<xs:complexType>
            <xs:sequence>
                <xs:element name="herdAlert" type="tns:HerdAlert"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>